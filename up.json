{
  "name": "ciri-auth",
  "profile": "lambda",
  "regions": ["us-west-2"],
  "hook": {
    "build": "yarn build"
  },
  "proxy": {
    "command": "node dist/server.js"
  },
  "stages": {
    "production": {
      "domain": "auth.memors.me"
    },
    "staging": {
      "domain": "auth-staging.memors.me"
    },
    "development": {
      "proxy": {
        "command": "nodemon index.js"
      }
    }
  }
}
